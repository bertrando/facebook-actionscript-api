package demo.events {		import flash.events.Event;			public class FacebookDemoEvent extends Event {				public static const USER_COMPLETE:String = 'userComplete';		public static const FRIENDS_COMPLETE:String = 'friendsComplete';		public static const FRIEND_SELECTED:String = 'friendSelected';		public static const IMAGE_COMPLETE:String = 'imageComplete';				public var data:Object = {};				public function FacebookDemoEvent(type:String, data:Object = null) {			// constructor code			super(type, false, false);			this.data = data;		}				override public function clone():Event {			return new FacebookDemoEvent(type, data);		}	}	}